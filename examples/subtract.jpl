/**
 * @brief Subtracts two floating-point numbers.
 *
 * @param a The first number.
 * @param b The second number.
 * @return a - b.
 */
fn subtract(a : float, b : float) : float {
    return a - b
}

show subtract(1000000000., 0.1)
